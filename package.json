{
  "name": "ejs-api",
  "version": "3.0.0",
  "description": "Express API",
  "author": "krishna",
  "main": "build/main.js",
  "scripts": {
    "build:dev": "npm run clean:dist && webpack --mode=development",
    "build:prod": "npm run clean:dist && webpack --mode=production",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:fix-dry": "eslint --ext .ts ./ --fix-dry-run",
    "prettier:fix": "npm run prettier:base -- --write \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "start": "nodemon -V --exec ts-node src/main.ts"
  },
  "keywords": [
    "typescript",
    "mocha",
    "sourcemaps",
    "node",
    "npm scripts",
    "tsc"
  ],
  "dependencies": {
    "@okta/jwt-verifier": "^1.0.0",
    "body-parser": "1.18.3",
    "compression": "1.7.2",
    "copy-webpack-plugin": "^6.0.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-winston": "2.5.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "nconf": "0.10.0",
    "path": "0.12.7",
    "rimraf": "2.6.2",
    "sequelize": "^6.2.0",
    "swagger-ui-express": "3.0.10",
    "tedious": "^8.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "2.8.4",
    "@types/express": "4.11.1",
    "@types/node": "10.3.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@webpack-cli/migrate": "^0.1.9",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-unicorn": "^20.1.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "ts-loader": "4.4.1",
    "ts-node": "6.1.0",
    "typescript": "^3.9.5",
    "webdriver-manager": "12.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "1.7.2",
    "winston": "2.4.2",
    "winston-daily-rotate-file": "3.2.1"
  }
}
