{
    "name": "ejs-api",
    "version": "1.0.0",
    "description": "Express API",
    "author": "krishna",
    "main": "build/app.js",
    "scripts": {
        "start": "nodemon --watch src/**/*.ts --exec ts-node src/main.ts",
        "lint": "npm run tslint \"src/**/*.ts\"",
        "tslint": "tslint",
        "pree2e": "webdriver-manager update",
        "build:prod": "npm run clean:dist && webpack --mode=production",
        "build:dev": "npm run clean:dist && webpack --mode=development",
        "clean:dist": "rimraf dist",
        "clean:build": "rimraf build"
    },
    "keywords": [
        "typescript",
        "mocha",
        "sourcemaps",
        "node",
        "npm scripts",
        "tsc"
    ],
    "dependencies": {
        "@types/body-parser": "1.17.0",
        "@types/compression": "0.0.36",
        "@types/cors": "2.8.4",
        "@types/express": "4.11.1",
        "@types/lodash": "4.14.109",
        "@types/node": "10.3.0",
        "agentkeepalive": "3.4.1",
        "async": "2.6.1",
        "body-parser": "1.18.3",
        "compression": "1.7.2",
        "cookie-parser": "1.4.3",
        "copy-webpack-plugin": "4.5.1",
        "cors": "2.8.4",
        "express": "4.16.3",
        "express-winston": "2.5.1",
        "fs": "0.0.1-security",
        "httpreq": "0.4.24",
        "jsonwebtoken": "8.2.2",
        "lodash": "4.17.10",
        "path": "0.12.7",
        "request": "2.87.0",
        "rimraf": "2.6.2",
        "sequelize": "4.37.10",
        "seriate": "0.13.1",
        "ts-loader": "4.4.1",
        "ts-node": "6.1.0",
        "tslint": "5.10.0",
        "typescript": "2.9.1",
        "webpack": "4.12.0",
        "webpack-cli": "3.0.3",
        "webpack-node-externals": "1.7.2",
        "winston": "2.4.2",
        "winston-daily-rotate-file": "3.2.1"
    },
    "devDependencies": {
        "@webpack-cli/migrate": "^0.0.8",
        "concurrently": "3.5.1",
        "convert-source-map": "1.5.1",
        "nodemon": "1.17.5",
        "webdriver-manager": "12.0.6"
    }
}