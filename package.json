{
  "name": "ejs-api",
  "version": "3.0.0",
  "description": "Express API",
  "author": "krishna",
  "main": "build/main.js",
  "scripts": {
    "build:dev": "npm run clean:dist && webpack --mode=development",
    "build:prod": "npm run clean:dist && webpack --mode=production",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "start": "nodemon -V --exec ts-node src/main.ts",
    "tslint": "tslint"
  },
  "keywords": [
    "typescript",
    "mocha",
    "sourcemaps",
    "node",
    "npm scripts",
    "tsc"
  ],
  "dependencies": {
    "@okta/jwt-verifier": "^1.0.0",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "2.8.4",
    "@types/express": "4.11.1",
    "@types/node": "10.3.0",
    "body-parser": "1.18.3",
    "compression": "1.7.2",
    "copy-webpack-plugin": "^6.0.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-winston": "2.5.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.3.0",
    "mssql": "^4.1.0",
    "nconf": "0.10.0",
    "path": "0.12.7",
    "rimraf": "2.6.2",
    "sequelize": "^6.1.0",
    "swagger-ui-express": "3.0.10",
    "tedious": "^8.3.0",
    "ts-loader": "4.4.1",
    "ts-node": "6.1.0",
    "tslint": "5.10.0",
    "typescript": "2.9.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "1.7.2",
    "winston": "2.4.2",
    "winston-daily-rotate-file": "3.2.1"
  },
  "devDependencies": {
    "@webpack-cli/migrate": "^0.1.9",
    "nodemon": "^2.0.4",
    "webdriver-manager": "12.0.6"
  }
}
